{
	order rate_limit before basicauth
}
:80 {
	redir /favicon.ico /favicon/favicon.ico 301
	root * /usr/share/caddy
	rate_limit {
		zone dynamic_zone {
			key static
			events 100
			window 1s
		}
	}
	encode zstd gzip
	reverse_proxy /smartProgress-or* web-ui.smart-progress-or.svc.cluster.local:8080
	file_server
	log
}
